<!-- Content Header (Page header) -->
<section class="content-header">
  <h1>
    Page Header
    <small>Optional description</small>
  </h1>
  <ol class="breadcrumb">
    <li><a href="<%= menus_path %>"><i class="fa fa-dashboard"></i>菜品列表</a></li>
    <li class="active">编辑菜品</li>
    <li><a href="<%= menu_path(@menu) %>">菜品详情</a></li>
  </ol>
</section>

<!-- Main content -->
<section class="content container-fluid">
  <div class="row">
    <!-- left column -->
    <div class="col-md-6">
      <!-- general form elements -->
      <div class="box box-primary">
        <div class="box-header with-border">
          <h3 class="box-title">编辑菜品</h3>
        </div>
        <!-- /.box-header -->
        <!-- form start -->
        <%= render 'form', menu: @menu %>
      </div>
    </div>
  </div>

</section>
